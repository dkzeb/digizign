{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "no-console": false,
        "member-ordering": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false 
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
          "*.json",
          "**/*.json"
        ]
      }
}